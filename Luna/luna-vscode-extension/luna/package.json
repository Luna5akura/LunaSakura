{
  "name": "luna",
  "publisher": "Luna5akura",
  "displayName": "LunaCode",
  "description": "Syntax Highlighter and Language Support for luna",
  "version": "0.0.6",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onLanguage:luna"
  ],
  "contributes": {
    "languages": [{
      "id": "luna",
      "aliases": ["Luna", "luna"],
      "extensions": [".luna"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "luna",
      "scopeName": "source.luna",
      "path": "./luna.tmLanguage.json"
    }]
  }
}