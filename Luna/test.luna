print "--- Testing List Creation & Push ---"
# 1. 创建列表
var numList = List()
print "Initial len:"
print len(numList) # 期望: 0

# 2. 添加元素 (Push)
push(numList, 10)
push(numList, 20)
push(numList, 30)
print "Len after push:"
print len(numList) # 期望: 3

print "--- Testing Get & Set ---"
# 3. 获取元素 (Get)
print "Index 0:"
print get(numList, 0) # 期望: 10
print "Index 1:"
print get(numList, 1) # 期望: 20

# 4. 修改元素 (Set)
set(numList, 1, 999)
print "Index 1 after set:"
print get(numList, 1) # 期望: 999

print "--- Testing Pop ---"
# 5. 弹出元素 (Pop)
var poppedVal = pop(numList)
print "Popped value:"
print poppedVal       # 期望: 30
print "Len after pop:"
print len(numList)    # 期望: 2

print "--- Testing Iteration ---"
# 6. 结合循环遍历列表
var idx = 0
var count = len(numList)
while idx < count:
    print get(numList, idx)
    idx = idx + 1
# 期望输出: 10 然后 999

print "--- Testing Clear ---"
# 7. 清空列表 (Clear)
clear(numList)
print "Len after clear:"
print len(numList) # 期望: 0

